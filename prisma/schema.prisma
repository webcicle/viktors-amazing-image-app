// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Image {
   id String @id @default(cuid())
   caption String
   created DateTime @default(now())
}

model Tag {
   id String @id @default(cuid())
   tagName String
}